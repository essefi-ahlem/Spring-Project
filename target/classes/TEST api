{
      	
"firstName"  : "FFFF ",	
"lastName"  : "FF ",	
"section"  	: " FF",
"CLASS" :{
                "id" : "6" , 
                "name" : "5555  ere Aa"
            }
}


PUT  :
{
"id"         :2 , 
"firstName"  : "FFFF ",	
"lastName"  : "FF ",	
"section"  	: " FF",
"CLASS" :{       "id" : 1  ,
                "name" : "5555  ere Aa"
            }
}






DELETE : 

http://localhost:8080/Students?STUDENT_ID=2








